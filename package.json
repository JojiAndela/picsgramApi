{
  "name": "picsgram",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/JojiAndela/picsgramApi.git",
  "author": "jojitoon",
  "license": "MIT",
  "scripts": {
    "start": "babel-node ./",
    "dev": "NODE_ENV=development nodemon index.js --exec babel-node",
    "pretest": "NODE_ENV=test npm run migrate",
    "test": "NODE_ENV=test nyc mocha test/index.js --require @babel/polyfill --require @babel/register --exit",
    "posttest": "NODE_ENV=test npm run unmigrate",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "createDB": "sequelize db:create",
    "migrate": "sequelize db:migrate",
    "unmigrate": "sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.0",
    "jsonwebtoken": "^8.3.0",
    "nodemon": "^1.18.6",
    "nyc": "^13.1.0",
    "pg": "^7.6.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.41.0",
    "sequelize-cli": "^5.3.0",
    "validator": "^10.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0"
  }
}
